#viewport x,y,width,height
viewport 120 5 834 650

# opened fsdb file
openFsdbFile /verdi/home/rich_chang/DAC/core_demo_2008/sim/novas.fsdb

addRuleTables
filterCriteria	Filter_Table_0
y	"Label"=="ADDA"	OR
y	"Label"=="TXA"	AND
endfilterCriteria
colorCriteria	Colorize_Table_0
